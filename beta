Tab:AddButton({
    Name = "循环红色球",
    Callback = function()
        local args = {
            [1] = "collectOrb",
            [2] = "Red Orb",
            [3] = "City"
        }
        local replicatedStorage = game:GetService("ReplicatedStorage")
        local orbEvent = replicatedStorage.rEvents.orbEvent
        local success, err = pcall(function()
            while true do
                orbEvent:FireServer(unpack(args))
                task.wait(0.3)
            end
        end)
        if not success then
            warn("Error:", err)
        end
    end
})

--/CREATE PART/
game.Players.PlayerAdded:Connect(function(player)
    player.CharacterAdded:Connect(function(character)
        local head = character:WaitForChild("Head")
        
        local part = Instance.new("Part")
        part.Name = "AboveHeadPart"
        part.Anchored = true
        part.Size = Vector3.new(1, 1, 1)
        part.Position = head.Position + Vector3.new(0, 1, 0)
        
        part.Parent = workspace
    end)
end)

--/NO MANDRAKES/
local conn
local function hideMandrake(obj)
    if obj and table.find(MandrakesNames, obj.Name) then
        obj.Parent = nil
        local scream = obj:FindFirstChild("ScreamSound")
        if scream then scream:Destroy() end
    end
end
   else
    CoreGui:SetCore("SendNotification", {
        Title = "Fluorine star error",
        Text = "用户:"..game.Players.LocalPlayer.Name.."无权限访问此脚本",
        Duration = 5
    })
    
    CoreGui:SetCore("SendNotification", {
        Title = "Error",
        Text = "Player id"..localPlayer.UserId,
        Duration = 5
    })
end

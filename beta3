        local Jumpscare3Folder = ReplicatedStorage:FindFirstChild("Jumpscare")
        if Jumpscare3Folder then
            Jumpscare3Folder:Destroy()
        end
    end
end)
ts:Toggle("门透视", false, function(state)
    DoorEsp = state
